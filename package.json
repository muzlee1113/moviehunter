    "concurrently": "^4.1.0",
    "connect-ensure-login": "^0.1.1",
    "cookie-parser": "^1.4.4",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-form-data": "^2.0.8",
    "express-session": "^1.16.1",
    "googleapis": "^39.2.0",
    "if-env": "^1.0.4",
    "mongoose": "^5.7.5",
